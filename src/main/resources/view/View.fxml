<HBox id="viewPane" prefHeight="600.0" prefWidth="896.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lowe.mike.strimko.controller.Controller">
