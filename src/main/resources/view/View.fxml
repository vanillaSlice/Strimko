                     </children>
                  </VBox>
               </content>
            </TitledPane>
            <TitledPane id="playPane" animated="false" maxWidth="1.7976931348623157E308" text="Play">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
               <content>
                  <VBox>
                     <children>
                        <ToggleButton id="solutionButton" fx:id="solutionButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#solutionButtonAction" text="Solution">
