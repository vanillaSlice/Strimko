                              <Insets bottom="10.0" />
                           </VBox.margin>
                        </ComboBox>
                        <ComboBox id="difficultyComboBox" fx:id="difficultyComboBox" maxWidth="1.7976931348623157E308" onAction="#difficultyComboBoxAction" promptText="Difficulty">
