<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox id="solveModeView" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefWidth="520.0" spacing="10.0"
	stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.65"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="lowe.mike.strimko.controller.SolveModeViewController">
	<children>
		<GridPane hgap="10.0" maxHeight="-Infinity" maxWidth="-Infinity"
			minHeight="-Infinity" minWidth="-Infinity" prefHeight="23.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<ComboBox id="typeComboBox" fx:id="typeComboBox" maxHeight="-Infinity"
					maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
					onAction="#typeComboBoxAction" prefWidth="120.0" promptText="Type"
					style="-fx-font-size: 12;" />
				<ComboBox id="sizeComboBox" fx:id="sizeComboBox" maxHeight="-Infinity"
					maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
					onAction="#sizeComboBoxAction" prefWidth="70.0" promptText="Size"
					style="-fx-font-size: 12;" GridPane.columnIndex="1" />
			</children>
		</GridPane>
		<GridPane id="gridPane" fx:id="gridPane" maxHeight="-Infinity"
			maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
			prefHeight="500.0" prefWidth="500.0" />
		<GridPane id="controlsPane" fx:id="controlsPane" alignment="CENTER_LEFT"
			prefHeight="56.0" vgap="10.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<HBox alignment="CENTER_LEFT" spacing="10.0">
					<children>
						<RadioButton id="streamsRadioButton" fx:id="streamsRadioButton"
							maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
							minWidth="-Infinity" mnemonicParsing="false" prefWidth="70.0"
							text="Streams">
							<font>
								<Font size="12.0" />
							</font>
							<toggleGroup>
								<ToggleGroup fx:id="streamsNumbersToggleGroup" />
							</toggleGroup>
						</RadioButton>
						<RadioButton id="numbersRadioButton" fx:id="numbersRadioButton"
							maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
							minWidth="-Infinity" mnemonicParsing="false" prefWidth="75.0"
							text="Numbers" toggleGroup="$streamsNumbersToggleGroup">
							<font>
								<Font size="12.0" />
							</font>
						</RadioButton>
						<StackPane maxHeight="-Infinity" maxWidth="-Infinity"
							minHeight="-Infinity" minWidth="-Infinity">
							<children>
								<GridPane id="streamsPane" fx:id="streamsPane"
									alignment="CENTER_LEFT" hgap="10.0" maxHeight="-Infinity"
									maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
									prefHeight="23.0" prefWidth="335.0" />
								<GridPane id="numbersPane" fx:id="numbersPane"
									alignment="CENTER_LEFT" hgap="10.0" maxHeight="-Infinity"
									maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
									prefHeight="23.0" prefWidth="335.0" />
							</children>
						</StackPane>
					</children>
				</HBox>
				<HBox alignment="CENTER_LEFT" prefHeight="23.0" spacing="10.0"
					GridPane.rowIndex="1">
					<children>
						<ToggleButton id="clearStreamButton" fx:id="clearStreamButton"
							maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
							minWidth="-Infinity" mnemonicParsing="false" prefWidth="90.0"
							text="Clear Stream">
							<font>
								<Font size="12.0" />
							</font>
							<toggleGroup>
								<ToggleGroup fx:id="controlsToggleGroup" />
							</toggleGroup>
						</ToggleButton>
						<ToggleButton id="clearNumberButton" fx:id="clearNumberButton"
							maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
							minWidth="-Infinity" mnemonicParsing="false" prefWidth="95.0"
							text="Clear Number" toggleGroup="$controlsToggleGroup">
							<font>
								<Font size="12.0" />
							</font>
						</ToggleButton>
						<ToggleButton id="solutionButton" fx:id="solutionButton"
							maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
							minWidth="-Infinity" mnemonicParsing="false" onAction="#solutionButtonAction"
							prefWidth="91.0" text="Solution" toggleGroup="$controlsToggleGroup">
							<font>
								<Font size="12.0" />
							</font>
						</ToggleButton>
						<Button id="saveButton" fx:id="saveButton" maxHeight="-Infinity"
							maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
							mnemonicParsing="false" onAction="#saveButtonAction" prefWidth="92.0"
							text="Save">
							<font>
								<Font size="12.0" />
							</font>
						</Button>
						<Button id="restartButton" fx:id="restartButton" maxHeight="-Infinity"
							maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
							mnemonicParsing="false" onAction="#restartButtonAction"
							prefWidth="92.0" text="Restart">
							<font>
								<Font size="12.0" />
							</font>
						</Button>
					</children>
				</HBox>
			</children>
		</GridPane>
	</children>
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
</VBox>