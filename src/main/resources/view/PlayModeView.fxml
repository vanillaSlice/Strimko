<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox id="playModeView" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefWidth="520.0" spacing="10.0"
	stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.65"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="lowe.mike.strimko.controller.PlayModeViewController">
	<children>
		<GridPane hgap="10.0" maxHeight="-Infinity" maxWidth="-Infinity"
			minHeight="-Infinity" minWidth="-Infinity" prefHeight="23.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<ComboBox id="typeComboBox" fx:id="typeComboBox" maxHeight="-Infinity"
					maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
					onAction="#typeComboBoxAction" prefWidth="120.0" promptText="Type"
					style="-fx-font-size: 12;" />
				<ComboBox id="difficultyComboBox" fx:id="difficultyComboBox"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" onAction="#difficultyComboBoxAction"
					prefWidth="120.0" promptText="Difficulty" style="-fx-font-size: 12;"
					GridPane.columnIndex="1" />
				<ComboBox id="nameComboBox" fx:id="nameComboBox" maxHeight="-Infinity"
					maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
					prefWidth="120.0" promptText="Name" style="-fx-font-size: 12;"
					GridPane.columnIndex="2" />
				<Button id="refreshButton" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
					onAction="#refreshButtonAction" prefWidth="23.0" styleClass="refreshButton"
					GridPane.columnIndex="3">
					<font>
						<Font size="12.0" />
					</font>
				</Button>
				<Button id="loadButton" fx:id="loadButton" maxHeight="-Infinity"
					maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
					mnemonicParsing="false" onAction="#loadButtonAction" prefWidth="77.0"
					text="Load" GridPane.columnIndex="4">
					<font>
						<Font size="12.0" />
					</font>
				</Button>
			</children>
		</GridPane>
		<GridPane id="gridPane" fx:id="gridPane" maxHeight="-Infinity"
			maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
			prefHeight="500.0" prefWidth="500.0" />
		<GridPane id="controlsPane" fx:id="controlsPane" alignment="CENTER_LEFT"
			prefHeight="56.0" vgap="10.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<HBox alignment="CENTER_LEFT" spacing="10.0">
					<children>
						<Label text="Numbers:">
							<font>
								<Font size="12.0" />
							</font>
						</Label>
						<GridPane id="numbersPane" fx:id="numbersPane" alignment="CENTER_LEFT"
							hgap="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
							minWidth="-Infinity" prefHeight="23.0" prefWidth="435.0" />
					</children>
				</HBox>
				<HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" prefHeight="23.0"
					spacing="10.0" GridPane.rowIndex="1">
					<children>
						<ToggleButton id="clearNumberButton" fx:id="clearNumberButton"
							maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
							minWidth="-Infinity" mnemonicParsing="false" prefWidth="95.0"
							text="Clear Number">
							<font>
								<Font size="12.0" />
							</font>
							<toggleGroup>
								<ToggleGroup fx:id="controlsToggleGroup" />
							</toggleGroup>
						</ToggleButton>
						<ToggleButton id="solveCellButton" fx:id="solveCellButton"
							maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
							minWidth="-Infinity" mnemonicParsing="false" prefWidth="75.0"
							text="Solve Cell" toggleGroup="$controlsToggleGroup">
							<font>
								<Font size="12.0" />
							</font>
						</ToggleButton>
						<ToggleButton id="pencilMarksButton" fx:id="pencilMarksButton"
							maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
							minWidth="-Infinity" mnemonicParsing="false" prefWidth="95.0"
							text="Pencil Marks">
							<font>
								<Font size="12.0" />
							</font>
						</ToggleButton>
						<ToggleButton id="hintButton" fx:id="hintButton"
							maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
							minWidth="-Infinity" mnemonicParsing="false" onAction="#hintButtonAction"
							prefWidth="55.0" text="Hint">
							<font>
								<Font size="12.0" />
							</font>
						</ToggleButton>
						<ToggleButton id="solutionButton" fx:id="solutionButton"
							maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
							minWidth="-Infinity" mnemonicParsing="false" onAction="#solutionButtonAction"
							prefWidth="65.0" text="Solution" toggleGroup="$controlsToggleGroup">
							<font>
								<Font size="12.0" />
							</font>
						</ToggleButton>
						<Button id="restartButton" fx:id="restartButton" maxHeight="-Infinity"
							maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
							mnemonicParsing="false" onAction="#restartButtonAction"
							prefWidth="65.0" text="Restart">
							<font>
								<Font size="12.0" />
							</font>
						</Button>
					</children>
				</HBox>
			</children>
		</GridPane>
	</children>
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
</VBox>